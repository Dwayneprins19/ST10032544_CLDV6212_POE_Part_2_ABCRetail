@{
	ViewData["Title"] = "Dequeue Message";
}

<h2 class="mb-3">Dequeue Message</h2>

@if (ViewBag.Message != null)
{
	<div class="alert alert-info">
		@ViewBag.Message
	</div>
}
else
{
	<div class="alert alert-warning">No messages available.</div>
}

<div class="mt-3">
	<a asp-action="Enqueue" class="btn btn-primary">Send Message</a>
	<a asp-action="Peek" class="btn btn-info">Peek Message</a>
	<a asp-action="All" class="btn btn-outline-dark">View All Messages</a>
</div>