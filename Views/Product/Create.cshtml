@model ABCRetail.Models.ProductEntity

<h2>Add Product</h2>
<form asp-action="Create" method="post" enctype="multipart/form-data">
	<label>Name</label>
	<input asp-for="Name"/>
	<label>Price:</label>
	<input asp-for="Price"/>
	<label>Image:</label>
	<input type="file" name="imageFile"/>
	<button type="submit">Add</button>
</form>